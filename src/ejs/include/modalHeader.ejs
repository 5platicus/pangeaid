<div class="modalHeader <%= identifier %>">
    <% if (back) { %>
        <i class="close-<%= identifier %> fal fa-chevron-left"></i>
    <% } else { %>
        <div></div>
    <% } %>
    <div>
        <i class="fal fa-file-contract"></i>
        <span class="modalTitle"><%= title %></span>
    </div>
    <i class="<%= closeClasses %> fal fa-times-circle"></i>
</div>
<div class="modalContent">
    <div class="formTitle">
        <h4><%= subTitle %></h4>    
    </div>