<div class="modalHeader <%= identifier %>">
    <% if (back) { %>
        <i class="back-<%= identifier %> fal fa-chevron-left headerNavButton"></i>
    <% } else { %>
        <div class="headerNavButton"></div>
    <% } %>
    <div class="modalTitleContainer">
        <i class="fal fa-file-contract"></i>
        <span class="modalTitle"><%= title %></span>
    </div>
    <i class="close-<%= identifier %> fal fa-times-circle headerNavButton" data-classes="<%= closeClasses %>"></i>
</div>
<div class="modalContent">
    <div class="formTitle">
        <h4><%= subTitle %></h4>    
    </div>