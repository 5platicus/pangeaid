<div class="modalHeader <%= identifier %>">
    <% if (back) { %>
        <i class="back-<%= identifier %> fal fa-chevron-left"></i>
    <% } else { %>
        <div></div>
    <% } %>
    <div>
        <i class="fal fa-file-contract"></i>
        <span class="modalTitle"><%= title %></span>
    </div>
    <i class="close-<%= identifier %> fal fa-times-circle" data-classes="<%= closeClasses %>"></i>
</div>
<div class="modalContent">
    <div class="formTitle">
        <h4><%= subTitle %></h4>    
    </div>